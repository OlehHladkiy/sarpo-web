{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "@config/*": ["config/*"],
      "@components/*": ["components/*"],
      "@graphql/*": ["graphql/*"],
      "@modules/*": ["modules/*"],
      "@services/*": ["services/*"],
      "@store/*": ["store/*"],
      "@theme/*": ["theme/*"]
    },
    "skipLibCheck": true
  },
  "include": ["./src/**/*"]
}